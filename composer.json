{
    "name": "stats4sd/laravel-ui",
    "description": "Stats4SD Fork of the Legacy Laravel UI package. Adds support for compiling front-end assets for Laravel Backpack",
    "keywords": ["laravel", "ui"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Mills",
            "email": "d.e.mills@stats4sd.org"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.42|^9.0",
        "illuminate/filesystem": "^8.42|^9.0",
        "illuminate/support": "^8.82|^9.0",
        "illuminate/validation": "^8.42|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stats4SD\\LaravelUi\\": "src/",
            "Illuminate\\Foundation\\Auth\\": "auth-backend/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Stats4SD\\LaravelUi\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
